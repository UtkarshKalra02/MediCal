{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "Patients": {
      "type": "string",
      "required": true
    },
    "YearExperience": {
      "type": "string",
      "required": true
    },
    "StartTime": {
      "type": "time"
    },
    "EndTime": {
      "type": "time"
    },
    "About": {
      "type": "richtext"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "doctor"
    },
    "Phone": {
      "type": "string",
      "required": true
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Premium": {
      "type": "boolean"
